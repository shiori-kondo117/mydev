<textarea name="F_SQLCOMMAND2" rows="5" cols="80">insert into T_録画記録(ディスクＩＤ,番組名,タイトル名,録画日付,録画時間,メモ) select ディスクＩＤ,番組名,タイトル名,録画日付,録画時間,メモ from l_録画記録</textarea>
